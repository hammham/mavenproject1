<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<AnchorPane prefWidth="600.0" prefHeight="400.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.mycompany.mavenproject1.SecondaryController">

    <children>
        <Label layoutX="50.0" layoutY="60.0" text="Secondary View" />
        <Button fx:id="secondaryButton" layoutX="50.0" layoutY="120.0" text="Switch to Primary View" onAction="#switchToPrimary" />
    </children>
</AnchorPane>
